<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:s="urn:import:havis.net.ui.shared.client">
	<ui:with type="havis.net.ui.middleware.client.shared.resourcebundle.ResourceBundle" field="res" />
	<ui:style>
	</ui:style>
	<g:HTMLPanel>
		<g:FlowPanel ui:field="lifeScale" >
			<g:Button ui:field="decrease" styleName="{res.css.webuiDecreaseButton}" />
			<g:FlowPanel ui:field="space" styleName="{res.css.scalefill}" />
			<g:Button ui:field="increase" styleName="{res.css.webuiIncreaseButton}" />
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 