<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:us="urn:import:havis.net.ui.shared.client"
	xmlns:srl="urn:import:havis.net.ui.middleware.client.shared.report.list"
	>
	
	<ui:with type="havis.net.ui.middleware.client.shared.resourcebundle.ResourceBundle" field="res" />
	<ui:with type="havis.net.ui.middleware.client.shared.resourcebundle.ConstantsResource" field="i18n" />
	
	<ui:style>
	</ui:style>
	<g:FlowPanel>
		<g:FlowPanel addStyleNames="{res.css.cycleAssocRow}">
			<g:Label text="{i18n.epc}" addStyleNames="{res.css.labelWidth}  {res.css.webuiProperty}" />
			<g:TextBox ui:field="key" addStyleNames="{res.css.nameinput} {res.css.webuiTextBox}" />
		</g:FlowPanel>
		<g:FlowPanel addStyleNames="{res.css.cycleAssocRow}">
			<g:Label text="{i18n.value}" addStyleNames="{res.css.labelWidth}  {res.css.webuiProperty}" />
			<g:TextBox ui:field="value" addStyleNames="{res.css.nameinput} {res.css.webuiTextBox}" />
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 