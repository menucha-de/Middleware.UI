<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:h="urn:import:havis.net.ui.middleware.client.shared"
	xmlns:l="urn:import:havis.net.ui.middleware.client.shared.trigger">
	<ui:with type="havis.net.ui.middleware.client.shared.resourcebundle.ResourceBundle" field="res" />
	<ui:style>
	</ui:style>
	<g:FlowPanel>
		<g:FlowPanel addStyleNames="{res.css.specOpRow}">
			<g:Label ui:text="Name" addStyleNames="{res.css.webuiProperty}"/>
			<g:TextBox ui:field="name" addStyleNames="{res.css.webuiTextBox}"  />
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 